<%= form_for @market do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :address %>
  <%= f.text_area :address %>

  <%= f.label :city %>
  <%= f.text_field :city %>

  <%= f.label :state %>
  <%= f.text_field :state %>

  <%= f.label :county %>
  <%= f.text_field :county %>

  <%= f.label :zip %>
  <%= f.text_field :zip %>

  <%= f.submit %>
<% end %>