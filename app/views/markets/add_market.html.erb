<%= render partial: "header" %>

<h1>Add new market</h1>

<%= form_for @market do |m| %>
 <fieldset>
   <legend>New task</legend>
   <%= m.label :name %>
   <%= m.text_field :name %>

   <%= m.label :address %>
   <%= m.text_field :address %>

   <%= m.label :city %>
   <%= m.text_field :city %>

   <%= m.label :county %>
   <%= m.text_field :county %>

   <%= m.label :state %>
   <%= m.text_field :state %>

   <%= m.label :zip %>
   <%= m.text_field :zip %>

 </fieldset>

 <%= m.submit 'Add new market' %>
<% end %>
