<h1> <%= @market.name%></h1>
<table>
  <thead>
    <tr>
      <td>
        Address
      </td>
      <td>
        City
      </td>
      <td>
        County
      </td>
      <td>
        State
      </td>
      <td>
        Zip Code
      </td>
    </tr>
  </thead>
  <tbody>
    <tr>
      <td>
        <%= @market.address %>
      </td>
      <td>
        <%= @market.city %>
      </td>
      <td>
        <%= @market.county %>
      </td>
      <td>
        <%= @market.state %>
      </td>
      <td>
        <%= @market.zip %>
      </td>
    </tr>
  </tbody>
</table>

<h2>Vendors associated with <%= @market.name %></h2>
<p>
  <a href=<%=  %>>+ Add a Vendor to <%= @market.name %></a>
</p>
<% @market.vendors.each do |vendor| %>
<li>
  <%= vendor.name %>
</li>
<% end %>
