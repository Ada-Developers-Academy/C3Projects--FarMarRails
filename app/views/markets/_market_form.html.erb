<%= form_for @market do |f| %>
  <fieldset class="form">

    <%= f.label :name %>
    <%= f.text_field :name, required: "required" %>

    <%= f.label :address %>
    <%= f.text_field :address %>

    <%= f.label :city %>
    <%= f.text_field :city %>

    <%= f.label :county %>
    <%= f.text_field :county %>

    <%= f.label :state %>
    <%= f.text_field :state %>

    <%= f.label :zip %>
    <%= f.text_field :zip %>

  </fieldset>
    <%=  f.submit %>
<% end %>
