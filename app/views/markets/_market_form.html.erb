<%= form_for @market do |f| %>

  <%= f.label :name %>
  <%= f.text_field :name, required: :true %>

  <%= f.label :address %>
  <%= f.text_field :address %>

  <%= f.label :city %>
  <%= f.text_field :city %>

  <%= f.label :state %>
  <%= f.text_field :state %>

  <%= f.label :zip %>
  <%= f.text_field :zip %>

  <%= f.submit %>

  <% end %>
