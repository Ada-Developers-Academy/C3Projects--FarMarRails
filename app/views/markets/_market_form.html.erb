<%= form_for @market do |f| %>
  <fieldset class="form">

  <div class="form_field">
    <%= f.label :name %>
    <%= f.text_field :name, required: "required" %>
  </div>

  <div class="form_field">
    <%= f.label :address %>
    <%= f.text_field :address %>
  </div>

  <div class="form_field">
    <%= f.label :city %>
    <%= f.text_field :city %>
  </div>

  <div class="form_field">
    <%= f.label :county %>
    <%= f.text_field :county %>
  </div>

  <div class="form_field">
    <%= f.label :state %>
    <%= f.text_field :state %>
  </div>

  <div class="form_field">
    <%= f.label :zip %>
    <%= f.text_field :zip %>
  </div>

</fieldset>
    <%=  f.submit %>
<% end %>
