<%= form_for @market do |f| %>
  <fieldset>
    <legend> Market Details </legend>
      <%= f.label :name %>
      <%= f.text_field :name %>

      <%= f.label :address %>
      <%= f.text_field :address %>

      <%= f.label :city %>
      <%= f.text_field :city %>

      <%= f.label :county %>
      <%= f.text_field :county %>

      <%= f.label :state %>
      <%= f.text_field :state %>

      <%= f.label :zip %>
      <%= f.date_field :zip %>

  </fieldset>
      <%= f.submit %>
<% end %>
