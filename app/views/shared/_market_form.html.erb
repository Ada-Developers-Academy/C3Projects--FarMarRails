
<%= form_for @market, url: @url, method: @method do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :address %>
  <%= f.text_field :address %>

  <%= f.label :city %>
  <%= f.text_field :city %>

  <%= f.label :state %>
  <%= f.select :state, options_for_select(us_states) %>

  <%= f.label :county %>
  <%= f.text_field :county %>

  <%= f.label :zip %>
  <%= f.text_field :zip %>

  <%= f.submit %>
<% end %>
