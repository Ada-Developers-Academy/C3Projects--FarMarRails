<%= form_for @vendor, url: url ||= '' do |f| %>
  <%= f.hidden_field(:market_id, value: params[:market_id]) %>
  
  <%= f.label :name %>
  <%= f.text_field :name %>
  <br><br>
  <%= f.label :number_of_employees %>
  <%= f.text_field :number_of_employees %>
  <br><br>
  <%= f.submit submit %>
<% end %>
