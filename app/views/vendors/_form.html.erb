<%= form_for @vendor, url: @url do |f| %>
  <fieldset>
    <legend> Vendor Details </legend>
      <%= f.label :name %>
      <%= f.text_field :name %>

      <%= f.label :num_employees , "Number of Employees" %>
      <%= f.text_field :num_employees %>

      <%= f.label :market_id %>
      <%= f.text_field :market_id %>

  </fieldset>
      <%= f.submit %>
<% end %>

